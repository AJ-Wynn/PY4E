score = input("Enter score: ")
try:
    float_score = float(score)
except:
    print("Bad score")
    exit()

if float_score < 0.0 or float_score > 1.0:
    print("Bad score")

elif float_score >= 0.9:
    print("A")

elif float_score >= 0.8:
    print("B")

elif float_score >= 0.7:
    print("C")

elif float_score >= 0.6:
    print("D")

elif float_score < 0.6:
    print("F")
